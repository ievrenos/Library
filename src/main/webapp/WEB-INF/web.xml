<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <listener>
        <description>ServletContextListener</description>
        <listener-class>edu.depaul.group2project.mostrecent.ServletListener</listener-class>
    </listener>
    <servlet>
        <servlet-name>AddBookServlet</servlet-name>
        <servlet-class>edu.depaul.group2project.mostrecent.AddBookServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddMemberServlet</servlet-name>
        <servlet-class>edu.depaul.group2project.mostrecent.AddMemberServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BookListServlet</servlet-name>
        <servlet-class>edu.depaul.group2project.mostrecent.BookListServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MemberInfoDisplayServlet</servlet-name>
        <servlet-class>edu.depaul.group2project.mostrecent.MemberInfoDisplayServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CheckOutBookServlet</servlet-name>
        <servlet-class>edu.depaul.group2project.mostrecent.CheckOutBookServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddBookServlet</servlet-name>
        <url-pattern>/AddBookServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddMemberServlet</servlet-name>
        <url-pattern>/AddMemberServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MemberInfoDisplayServlet</servlet-name>
        <url-pattern>/MemberInfoDisplayServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CheckOutBookServlet</servlet-name>
        <url-pattern>/CheckOutBookServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BookListServlet</servlet-name>
        <url-pattern>/BookListServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
